#include <iostream>
using namespace std;
int main(){
	int n;
	cout<<"enter the length of array : ";
	cin>>n;
	int arr[n] ;
	cout<<"enter all values : \n";
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	    arr[i] = pow(arr[i],2);
	  
	}
	cout<<"square values of your array\n";
	for(int i=0;i<n;i++){
	    cout<<arr[i]<<endl;
	}
}
