#include <iostream>
using namespace std;
int main(){
	int r,c;
	cout<<"enter the number of rows : ";
	cin>>r;
	cout<<"enter the number of column : ";
	cin>>c;
	cout<<"enter element of matrix 1:";
	int arr[r][c];
	int brr[r][c];
    int crr[r][c];
	for(int i=0;i<r;i++){
	    for(int j=0;j<c;j++){
	        cin>>arr[i][j];
	    }
	}
	cout<<"enter element of matrix 2 :";
    
    for(int i=0;i<r;i++){
	    for(int j=0;j<c;j++){
	        cin>>brr[i][j];
	    }
	}
	cout<<"matrix 1 :-\n";
    for(int i=0;i<r;i++){
	    for(int j=0;j<c;j++){
	        cout<<arr[i][j]<<"  ";
	    }
	    cout<<endl;
	    }
	
	cout<<"matrix 2 :-\n";
    for(int i=0;i<r;i++){
	    for(int j=0;j<c;j++){
	        cout<<brr[i][j]<<"  ";
	    }
	    cout<<endl;
	    
    }
    
       
    for(int i=0;i<r;i++){
	    for(int j=0;j<c;j++){
	        crr[i][j] = arr[i][j]+brr[i][j];
	    }
	}
	cout<<"sum matrix :-\n";
     for(int i=0;i<r;i++){
	    for(int j=0;j<c;j++){
	        cout<<crr[i][j]<<"  ";
	    }
	    cout<<endl;
	    }
	    for(int i=0;i<r;i++){
	    for(int j=0;j<c;j++){
	        crr[i][j] = arr[i][j]-brr[i][j];
	    }
	}
	cout<<"diffrence matrix :-\n";
     for(int i=0;i<r;i++){
	    for(int j=0;j<c;j++){
	        cout<<crr[i][j]<<"  ";
	    }
	    cout<<endl;
	    }
	    
	  
}
