#include<iostream>
using namespace std;
int main(){
    int x,y;
    cout<<"enter 1st number : ";
    cin>>x;
    cout<<"enter 2nd number :";
    cin>>y;
    cout<<"sum is"<<x+y<<endl;
    cout<<"difference is"<<x-y<<endl;
    cout<<"multiplication is "<<x*y<<endl;
    cout<<"division is "<<(float)x/y<<endl;
    }
