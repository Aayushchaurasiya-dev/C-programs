#include <iostream>
using namespace std;
int main() {
    int x,y;
    cout<<"enter 1st number : ";
    cin>>x;
    cout<<"enter 2nd number : ";
    cin>>y;
    cout<<"original values"<<endl;
    cout<<x<<" "<<y<<endl;
    int* p = &x;
    int* c = &y;
    int temp = *p ;
    *p= y;
    *c = temp;
    cout<<"swap values"<<endl;
     cout<<x<<" "<<y;
}
